{
  "name": "jupyterhub_singleuser_profiles_ui",
  "version": "0.1.0",
  "description": "JupyterHub Spawner UI",
  "author": "Red Hat",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/red-hat-data-services/jupyterhub-singleuser-profiles"
  },
  "homepage": "https://github.com/red-hat-data-services/jupyterhub-singleuser-profiles/",
  "bugs": {
    "url": "https://github.com/red-hat-data-services/jupyterhub-singleuser-profiles/issues"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "run-s build:prod",
    "build:for-deploy": "react-scripts build",
    "build:analyze": "export _ODH_OUTPUT_ONLY=true; run-s build build:bundle-profile build:bundle-analyze",
    "build:bundle-profile": "webpack --config ./config/webpack.prod.js --profile --json > ./bundle.stats.json",
    "build:bundle-analyze": "webpack-bundle-analyzer ./bundle.stats.json",
    "build:clean": "rimraf ./public",
    "build:prod": "webpack --config ./config/webpack.prod.js",
    "format": "prettier --write \"./**/*.ts\" \"./**/*.tsx\" \"./**/*.scss\"",
    "start:dev": "webpack serve --hot --color --config ./config/webpack.dev.js",
    "test:fix": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src --fix",
    "test:lint": "eslint --ext .json,.js,.ts,.jsx,.tsx ./src"
  },
  "dependencies": {
    "@patternfly/patternfly": "4.87.3",
    "@patternfly/react-core": "4.97.2",
    "@patternfly/react-icons": "4.9.2",
    "@patternfly/react-styles": "4.8.2",
    "@patternfly/react-table": "^4.23.2",
    "@patternfly/react-tokens": "^4.10.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.7",
    "classnames": "^2.2.6",
    "dompurify": "^2.2.6",
    "node-gyp": "^7.1.2",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^4.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "copy-webpack-plugin": "^6.2.1",
    "css-loader": "^5.0.0",
    "dotenv": "^8.2.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-react": "^7.21.4",
    "eslint-plugin-react-hooks": "^4.1.2",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "^4.5.0",
    "imagemin": "^7.0.0",
    "mini-css-extract-plugin": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "null-loader": "^4.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "raw-loader": "^4.0.2",
    "react-docgen-typescript-loader": "^3.7.2",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "sass-loader": "^7.3.1",
    "style-loader": "^2.0.0",
    "svg-url-loader": "^6.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.5",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.3",
    "typescript": "^4.0.3",
    "url-loader": "^4.1.1",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "^5.2.0"
  }
}
